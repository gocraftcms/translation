{
    "name": "panlatent/craft-translator",
    "description": "Translator is a plugin to edit your i18n translations for Craft 3",
    "type": "craft-plugin",
    "version": "0.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "translator"
    ],
    "support": {
        "docs": "https://github.com/panlatent/craft-translator/blob/master/README.md",
        "issues": "https://github.com/panlatent/craft-translator/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "panlatent",
            "email": "panlatent@gmail.com",
            "homepage": "https://panlatent.com"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0"
    },
    "autoload": {
        "psr-4": {
          "panlatent\\craft\\translator\\": "src/"
        }
    },
    "extra": {
        "name": "Translator",
        "handle": "translator",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/panlatent/craft-translator/master/CHANGELOG.md",
        "class": "panlatent\\craft\\translator\\Translator"
    }
}
