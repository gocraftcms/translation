{
    "name": "gocraft/translation",
    "description": "Translation is a plugin to edit your i18n translations for Craft 3",
    "type": "craft-plugin",
    "version": "0.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "translation",
        "i18n",
        "translator"
    ],
    "support": {
        "docs": "https://github.com/gocraftcms/translation/blob/master/README.md",
        "issues": "https://github.com/gocraftcms/translation/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "panlatent",
            "email": "panlatent@gmail.com",
            "homepage": "https://panlatent.com"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0"
    },
    "autoload": {
        "psr-4": {
          "gocraft\\translator\\": "src/"
        }
    },
    "extra": {
        "name": "Translation",
        "handle": "translation",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/gocraftcms/translation/master/CHANGELOG.md",
        "class": "gocraft\\translation\\Translation"
    }
}
